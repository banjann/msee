setwd("~/RStudioDirectory/Barplots/Mondays")
a <- import("setpoint27.csv")
library(ggplot2)
library(rio)
a <- import("setpoint27.csv")
ggplot(a, aes(factor(time), kw, fill = Demand)) +
geom_bar(stat = "identity", position = "dodge", col = "skyblue4") +
geom_text(aes(label = round(kw, 1)), position = position_dodge(0.9),
vjust = 1.5, color = "black", size = 2.25) +
scale_fill_brewer(palette = "Pastel1") +
theme(axis.text.x = element_text(angle=65, vjust=1)) +
labs(title="Average Measured and Simulated Power Demand",
subtitle = "All Mondays of April 2019",
x="Time",
y="Power in kW")
