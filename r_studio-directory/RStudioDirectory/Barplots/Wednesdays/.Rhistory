library(ggplot2)
library(ggpubr)
library(rio)
a <- import("barplot15.csv")
setwd("~/RStudioDirectory/BarPlot_SimuMeasured/Wednesdays")
a <- import("barplot15.csv")
theme_set(theme_bw())
m1<- ggplot(a, aes(factor(time), kw, fill = demand)) +
